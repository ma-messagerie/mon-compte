<?php
session_start();
ini_set('max_execution_time', 30); // S√©curit√© contre les boucles infinies de log

// üö® CORRECTION CORS : Autorise les requ√™tes de tous les domaines (*)
header("Access-Control-Allow-Origin: *"); 
header("Access-Control-Allow-Methods: GET, POST");

// ============================================================
// 1. CONFIGURATION GLOBALE & MATRICE DES OFFRES
// ============================================================

$log_filename_geo = "logs_geo_daily.txt";
$log_filename_views = "views_log.jsonl"; // Log des vues d'offres (d√©duplication)

// Dur√©e du cache en m√©moire (en secondes)
const CACHE_TTL_SECONDS = 300; // 5 minutes
const LOG_CLEANUP_CHANCE = 100; // 1 chance sur 100 de lancer le nettoyage du log

// --- MATRICE DES OFFRES COMPL√àTE ---
// Structure: [ID] => [URL, Type, Reset_Hours, [Sources]]
// üö® NOTE : '/default' est ajout√© temporairement √† toutes les sources pour le d√©bogage.
$matrix_offres = [
    // --- CPL (Reset 36h) ---
	'cpl_45' => "https://www.ever-trck.com/W57NTX/2CTPL/?uid=45&sub1=%EMAIL%",
    'cpl_43' => "https://www.ever-trck.com/W57NTX/9B9DM/?uid=43&sub1=%EMAIL%",
    'cpl_62' => "https://www.ever-trck.com/W57NTX/FGXLG/?uid=62&sub1=%EMAIL%",
    'cpl_clemzer' => "https://www.affizer-dating.com/redirect/00088?aff_sub=%EMAIL%",
    
    // --- CPC (Reset 1min / 0.0166h) ---
    'cpl_lb' => ["https://ab.pdtrcksus.com/v1/redirect/34004?utm_term=brevo&email=%EMAIL%", 'CPC', 0.0166, ['/Webcam', '/Question', '/Like', '/default']],
	'cpl_lb2' => ["https://ab.pdtrcksus.com/v1/redirect/34004?utm_term=brevo&email=%EMAIL%", 'CPC', 0.0166, ['/Webcam', '/Question', '/Like', '/default']],
    
    // --- CPA (Reset 1h) ---
    'cpa_lb1' => ["https://ab.pdtrcksus.com/v1/redirect/35449?utm_term=ACgourg&email=%EMAIL%", 'CPA', 1, ['/MP', '/Localisation', '/default']],
    'cpa_lb2' => ["https://ab.pdtrcksus.com/v1/redirect/35449?utm_term=ACgourg&email=%EMAIL%", 'CPA', 1, ['/MP', '/Localisation', '/default']],
];

// D√©finition des constantes HTML/CSS n√©cessaires pour le template (conserv√©)
const SUCCESS_ICON = '<svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#009432" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 6 9 17 4 12"></polyline></svg>';
const LOADER_HTML = '<div id="loader" style="width: 14px; height: 14px; border: 2px solid #f3f3f3; border-top: 2px solid #4285f4; border-radius: 50%; animation: spin 0.8s linear infinite; display: none;"></div>';
const LOADER_CSS = '@keyframes spin { 100% { transform: rotate(360deg); } }';


// ============================================================
// 2. FONCTIONS DE GESTION DES LOGS PERSISTANTS & CACHE
// ============================================================

/**
 * D√©termine la source du trafic (le chemin Github).
 * @return string Le chemin normalis√© (ex: "/mp" ou "/annonce").
 */
function getTrafficSource() {
    $referer = $_SERVER['HTTP_REFERER'] ?? '';
    
    // V√©rification stricte du REFERER GitHub pour extraire le chemin.
    if (preg_match('/github\.io(\/[^\/\?#]+)/i', $referer, $matches)) {
        return strtolower($matches[1]); // Ex: /mp
    }
    
    // FALLBACK : Source par d√©faut (doit √™tre ajout√©e √† toutes les offres pour les tests)
    return '/default';
}

/**
 * R√©cup√®re les vues bloqu√©es (non expir√©es) pour l'email donn√©.
 * G√®re le cache de session (H+5min) et la lecture du Log Disque.
 */
function getBlockedViewsFromLog($email) {
    global $log_filename_views;
    
    $email_hash = hash('sha256', $email);
    $current_time = time();
    $blocked_views = [];

    // --- 1. ESSAI DU CACHE DE SESSION (H+5min) ---
    if (isset($_SESSION['cache_offers'][$email_hash]) && 
        $_SESSION['cache_offers'][$email_hash]['ts_expiry'] > $current_time) 
    {
        return $_SESSION['cache_offers'][$email_hash]['blocked_list'];
    }

    // --- 2. LECTURE DU LOG DISQUE (si cache expir√© ou absent) ---
    $log_content = @file_get_contents(__DIR__ . '/' . $log_filename_views);
    if ($log_content) {
        $lines = explode("\n", trim($log_content));
        
        foreach ($lines as $line) {
            $data = json_decode($line, true);
            if ($data && ($data['email_hash'] ?? '') === $email_hash) {
                
                $offer_id = $data['offer_id'];
                $ts_view = $data['ts_view'];
                $reset_h = $data['reset_h'] ?? 0;
                
                if ($reset_h > 0) {
                    $reset_s = $reset_h * 3600; 
                    $ts_expiry = $ts_view + $reset_s;
                    
                    if ($ts_expiry > $current_time) {
                        $blocked_views[$offer_id] = $ts_view;
                    }
                } else {
                    // Reset_h = 0 (Bloqu√© √† vie)
                    $blocked_views[$offer_id] = $ts_view;
                }
            }
        }
    }
    
    // --- 3. MISE EN CACHE DE SESSION (pour les 5 prochaines minutes) ---
    $_SESSION['cache_offers'][$email_hash] = [
        'ts_expiry' => $current_time + CACHE_TTL_SECONDS,
        'blocked_list' => $blocked_views
    ];
    
    // --- 4. TENTATIVE DE NETTOYAGE DU LOG (1 chance sur X) ---
    if (mt_rand(1, LOG_CLEANUP_CHANCE) === 1) {
        logGarbageCollection();
    }

    return $blocked_views;
}

/**
 * Enregistre une vue d'offre dans le log persistant et met √† jour le cache de session.
 */
function logOfferView($offerId, $resetH) {
    global $log_filename_views;
    
    $email = $_SESSION['saved_email'] ?? 'unknown';
    if ($email === 'unknown') return;

    $data = [
        'email_hash' => hash('sha256', $email),
        'offer_id'   => $offerId,
        'ts_view'    => time(),
        'reset_h'    => $resetH 
    ];
    
    // √âcriture rapide et s√©curis√©e (append)
    $json_line = json_encode($data);
    file_put_contents(__DIR__ . '/' . $log_filename_views, $json_line . "\n", FILE_APPEND | LOCK_EX);
    
    // üö® MISE √Ä JOUR IMM√âDIATE DU CACHE DE SESSION
    $email_hash = $data['email_hash'];
    if (isset($_SESSION['cache_offers'][$email_hash])) {
        // Ajout de la nouvelle vue √† la liste bloqu√©e en m√©moire
        $_SESSION['cache_offers'][$email_hash]['blocked_list'][$offerId] = $data['ts_view'];
        // Reset du timer √† 5 minutes
        $_SESSION['cache_offers'][$email_hash]['ts_expiry'] = time() + CACHE_TTL_SECONDS;
    } else {
        // Si le cache n'existait pas pour une raison quelconque (multi-session), on le rafra√Æchit
        getBlockedViewsFromLog($email); 
    }
}

/**
 * Nettoie le fichier views_log.jsonl en retirant les entr√©es expir√©es (H+Reset).
 */
function logGarbageCollection() {
    global $log_filename_views;
    
    $log_path = __DIR__ . '/' . $log_filename_views;
    if (!file_exists($log_path)) return;

    $new_content = '';
    $current_time = time();

    $handle = @fopen($log_path, "r");
    if ($handle) {
        while (($line = fgets($handle)) !== false) {
            $data = json_decode(trim($line), true);
            
            if ($data) {
                $ts_view = $data['ts_view'] ?? 0;
                $reset_h = $data['reset_h'] ?? 0;
                
                $is_expired = false;
                if ($reset_h > 0) {
                    $reset_s = $reset_h * 3600;
                    if (($ts_view + $reset_s) <= $current_time) {
                        $is_expired = true; // Expir√©
                    }
                }
                
                if (!$is_expired) {
                    $new_content .= $line;
                }
            }
        }
        fclose($handle);
        
        // R√©√©crire uniquement si la taille est significativement diff√©rente (√©conomie d'op√©rations disque)
        $old_size = filesize($log_path);
        $new_size = strlen($new_content);

        // Si la nouvelle taille est inf√©rieure √† l'ancienne (nettoyage r√©ussi)
        if ($new_size < $old_size) { 
            file_put_contents($log_path, $new_content, LOCK_EX);
        }
    }
}

function getRealIp() {
    if (!empty($_SERVER['HTTP_CLIENT_IP'])) return $_SERVER['HTTP_CLIENT_IP'];
    if (!empty($_SERVER['HTTP_X_FORWARDED_FOR'])) return explode(',', $_SERVER['HTTP_X_FORWARDED_FOR'])[0];
    return $_SERVER['REMOTE_ADDR'] ?? 'NA';
}

/**
 * Pr√©pare l'URL finale en rempla√ßant %EMAIL% et ajoutant les subIDs.
 */
function prepareUrl($rawUrl) {
    $email = $_SESSION['saved_email'] ?? ''; 
    $url = str_replace('%EMAIL%', rawurlencode($email), $rawUrl);
    
    // Gestion de %CHEMIN% (param√®tres suppl√©mentaires de l'URL source)
    if (strpos($url, '%CHEMIN%') !== false) {
        $extra_params = $_SESSION['extra_params'] ?? [];
        if (!empty($extra_params)) {
            $url = str_replace('%CHEMIN%', http_build_query($extra_params), $url);
        } else {
            $url = str_replace('&%CHEMIN%', '', $url);
        }
    }
    
    return $url;
}

/**
 * Enregistre l'action compl√®te (Geo/IP/UA) dans le log quotidien et g√®re le reset √† minuit.
 * Cette fonction est optimis√©e pour cr√©er UNE SEULE LIGNE par email par jour.
 */
function performGeoLoggingAndReset($targetUrl) {
    global $log_filename_geo;
    
    $email = $_SESSION['saved_email'] ?? 'unknown';
    $log_path = __DIR__ . "/" . $log_filename_geo;
    
    // --- RESET QUOTIDIEN VIA FICHIER DRAPEAU ---
    $reset_flag_file = __DIR__ . "/.log_reset_flag";
    $today_date = date('Y-m-d');
    
    $last_reset_date = @file_get_contents($reset_flag_file);
    
    if ($last_reset_date !== $today_date) {
        file_put_contents($log_path, '', LOCK_EX);
        file_put_contents($reset_flag_file, $today_date, LOCK_EX);
        unset($_SESSION['logged_emails_today']); 
    }
    
    // --- üö® CORRECTION : V√âRIFICATION DIRECTE DANS LE FICHIER (anti-race condition) ---
    if ($email !== 'unknown') {
        // Ouvrir le fichier avec verrou exclusif pour √©viter les √©critures simultan√©es
        $fp = fopen($log_path, 'a+');
        if ($fp && flock($fp, LOCK_EX)) { // Verrou exclusif
            
            // Relire le contenu pour v√©rifier si l'email existe d√©j√† aujourd'hui
            rewind($fp);
            $content = stream_get_contents($fp);
            
            if (strpos($content, "EMAIL:$email") !== false) {
                // L'email est d√©j√† dans le log d'aujourd'hui ‚Üí on arr√™te
                flock($fp, LOCK_UN);
                fclose($fp);
                return;
            }
            
            // --- COLLECTE DES DONN√âES (inchang√©) ---
            $date = date('Y-m-d H:i:s');
            $ip = getRealIp();
            $userAgentFull = $_SERVER['HTTP_USER_AGENT'] ?? 'Unknown Agent';
            
            $protocol = (isset($_SERVER['HTTPS']) && $_SERVER['HTTPS'] === 'on') ? "https" : "http";
            $host = $_SERVER['HTTP_HOST'] ?? 'lespetitscoquins.fr';
            $uri = $_SERVER['REQUEST_URI'] ?? '';
            $sourceUrl = $_SERVER['HTTP_REFERER'] ?? $protocol . "://" . $host . $uri;

            $country='NA'; $city='NA'; $zip='NA'; $region='NA'; $dept='NA'; $isp='NA';
            $conn_type = 'NA'; $device = 'Ordinateur';
            
            if (preg_match('/mobile|android|iphone/i', $userAgentFull)) $device = 'Mobile';
            elseif (preg_match('/ipad|tablet/i', $userAgentFull)) $device = 'Tablette';

            $callApi = function($u){ 
                $c=curl_init(); curl_setopt($c,CURLOPT_URL,$u); curl_setopt($c,CURLOPT_RETURNTRANSFER,1); 
                curl_setopt($c,CURLOPT_TIMEOUT,3); 
                curl_setopt($c, CURLOPT_SSL_VERIFYHOST, 0); curl_setopt($c, CURLOPT_SSL_VERIFYPEER, 0);
                $d=curl_exec($c); curl_close($c); 
                return $d?json_decode($d,true):null; 
            };

            $d = $callApi("http://ip-api.com/json/$ip?fields=status,country,city,zip,regionName,isp,mobile");
            
            if ($d && ($d['status']??'')==='success') {
                $country = $d['country'] ?? 'NA';
                $city    = $d['city']    ?? 'NA';
                $zip     = $d['zip']     ?? 'NA';
                $region  = $d['regionName'] ?? 'NA';
                $isp     = $d['isp']     ?? 'NA';
                
                $is_cellular_ip = $d['mobile'] ?? false;
                if ($is_cellular_ip) {
                    $conn_type = 'Cellulaire (4G/5G)';
                } else {
                    $conn_type = ($device === 'Mobile' || $device === 'Tablette') ? 'WiFi' : 'Fixe (Ethernet/WiFi)';
                }
                
                if ($country === 'France' && $zip !== 'NA') $dept = substr($zip, 0, 2);

            } else {
                $d = $callApi("http://ipinfo.io/$ip/json");
                if ($d && !isset($d['error'])) {
                    $country = $d['country'] ?? 'NA';
                    if($country == 'FR') $country = 'France';
                    $city    = $d['city']    ?? 'NA';
                    $zip     = $d['postal']  ?? 'NA';
                    $region  = $d['region']  ?? 'NA';
                    $isp     = $d['org']     ?? 'NA';
                    $conn_type = 'Inconnu (Backup)';
                    
                    if ($country === 'France' && $zip !== 'NA') $dept = substr($zip, 0, 2);
                }
            }

            // --- √âCRITURE (sous verrou) ---
            $logData = "$date | IP:$ip | EMAIL:$email | PAYS:$country | REGION:$region | DEPT:$dept | VILLE:$city | ZIP:$zip | FAI:$isp | CONNEXION:$conn_type | SUPPORT:$device | DEPUIS:$sourceUrl | VERS:$targetUrl\n";
            
            fwrite($fp, $logData);
            
            // Marquer dans la session aussi (pour √©viter les appels API inutiles)
            if (!isset($_SESSION['logged_emails_today'])) {
                $_SESSION['logged_emails_today'] = [];
            }
            $_SESSION['logged_emails_today'][$email] = true;
            
            flock($fp, LOCK_UN); // Lib√©rer le verrou
            fclose($fp);
        }
    }
}

// ============================================================
// 3. LOGIQUE DE S√âLECTION DES OFFRES
// ============================================================

/**
 * Impl√©mente la s√©lection des 3 offres purement al√©atoires parmi les disponibles.
 * G√®re le filtre par Source, le H+Reset et l'√©puisement de cat√©gorie.
 */
function selectThreeRandomOffers() {
    global $matrix_offres;
    
    $email = $_SESSION['saved_email'] ?? 'unknown';
    $source = getTrafficSource(); // Ex: /Annonce, /MP
    
    // üö® D√âBOGAGE TEMPORAIRE üö® (√Ä retirer apr√®s les tests)
    if ($email === 'unknown') {
         return ['status' => 'error', 'message' => 'DEBUG: Email manquant ou invalide. Source: ' . $source . '. (Assurez-vous que l\'URL contient ?u={email})'];
    }
    
    // --- 1. Filtrer les offres par la Source (Source Valide) ---
    $valid_offers = [];
    foreach ($matrix_offres as $id => $data) {
        $sources = $data[3];
        if (in_array($source, $sources, true)) {
            $valid_offers[$id] = $data;
        }
    }
    
    if (empty($valid_offers)) {
        return ['status' => 'error', 'message' => 'No valid offers for source: ' . $source];
    }
    
    // --- 2. Filtrer par VUES BLOQU√âES (H+Reset) ---
    $blocked_views = getBlockedViewsFromLog($email);
    $available_offers = [];

    // Offres disponibles (non dans $blocked_views)
    foreach (array_keys($valid_offers) as $id) {
        if (!isset($blocked_views[$id])) {
            $available_offers[$id] = $valid_offers[$id];
        }
    }

    // --- 3. GESTION DE L'√âPUISEMENT DE CAT√âGORIE (Forcer le reset) ---
    if (count($available_offers) < 3) {
        
        $eligible_blocked = [];
        // 3a. R√©cup√©rer toutes les offres VALIDES pour cette source qui sont BLOQU√âES
        foreach (array_keys($valid_offers) as $id) {
            if (isset($blocked_views[$id])) {
                $eligible_blocked[$id] = $blocked_views[$id]; // [ID => Timestamp_Vue]
            }
        }
        
        // 3b. Tri par anciennet√© (le plus ancien d'abord)
        if (!empty($eligible_blocked)) {
            asort($eligible_blocked); 
            
            $needed = 3 - count($available_offers);
            
            $offers_to_reset = array_keys(array_slice($eligible_blocked, 0, $needed, true));
            
            foreach ($offers_to_reset as $id) {
                // Remettre l'offre bloqu√©e la plus ancienne dans le pool disponible (rotation)
                $available_offers[$id] = $valid_offers[$id];
            }
        }
    }
    
    // --- 4. S√âLECTION PUREMENT AL√âATOIRE des 3 finalistes ---
    $final_pool = array_keys($available_offers);
    
    // Si nous n'avons pas 3 offres diff√©rentes m√™me apr√®s le reset forc√© (DUPLICATION)
    if (count($final_pool) < 3) {
         $final_keys = [];
         // Dupliquer la ou les offres disponibles pour obtenir 3 slots
         while (count($final_keys) < 3 && !empty($final_pool)) {
             // Utiliser shuffle pour garder l'aspect al√©atoire si plusieurs offres existent
             shuffle($final_pool); 
             $final_keys = array_merge($final_keys, $final_pool);
         }
         $final_keys = array_slice($final_keys, 0, 3);
         
    } else {
        // S√©lection al√©atoire normale
        $final_keys = array_rand(array_flip($final_pool), 3);
        if (!is_array($final_keys)) $final_keys = [$final_keys];
    }

    // --- 5. ENREGISTREMENT ET PR√âPARATION DES LIENS ---
    $urls = [];
    foreach ($final_keys as $key) {
        if (!isset($matrix_offres[$key])) continue;

        $offer_data = $matrix_offres[$key];
        $url = prepareUrl($offer_data[0]);
        $reset_h = $offer_data[2];
        
        // LOG l'action pour bloquer les futures vues (H+Reset)
        if ($email !== 'unknown') {
            logOfferView($key, $reset_h);
        }

        // Attribution des slots de redirection
        if (!isset($urls['url_main'])) {
            $urls['url_main'] = $url;
        } elseif (!isset($urls['url_pop'])) {
            $urls['url_pop'] = $url;
        } else {
            // Pour le trap, on renvoie l'ID (le JS le transformera en URL avec ?sec_id)
            $urls['id_back'] = $key;
        }
    }
    
    // S'assurer que les 3 slots sont remplis (fallbacks)
    $urls['url_main'] = $urls['url_main'] ?? $urls['url_pop'] ?? 'https://fallback-main.com';
    $urls['url_pop'] = $urls['url_pop'] ?? $urls['url_main'] ?? 'https://fallback-pop.com';
    $urls['id_back'] = $urls['id_back'] ?? 'offrefr'; // ID d'une offre existante pour le trap

    return ['status'=>'ok', 'url_main'=>$urls['url_main'], 'url_pop'=>$urls['url_pop'], 'id_back'=>$urls['id_back']];
}

